      "required": true
    },
    {
      "name": "numberOfQuestions",
      "description": "Number of Questions to regenerate.",
      "required": true
    },
    {
      "name": "questionTypes",
      "description": "Type of questions to regenerate.",
      "required": true
    }
  ]
}